<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="Marriot-SwapTokens-Target">
    <Description>Marriot-SwapTokens-Target</Description>
	<FaultRules>
	</FaultRules>
	<DefaultFaultRule>
            <Step>
                <Name>SF-PostflowResponseTargetLogAggregator</Name>
            </Step>
		<Step>
            <Name>SF-Noname-Security</Name>
    	</Step>
    	<AlwaysEnforce>true</AlwaysEnforce>
	</DefaultFaultRule>
	<PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>SF-PreflowRequestTargetLogAggregator</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
	<Flows>
        <Flow name="partner-status">
        	<Condition>(targetOperation = &quot;partner-status&quot;)</Condition>
			<Request>
			</Request>   
			<Response>
			</Response>
        </Flow>
        <Flow name="auth-tokens">
        	<Condition>(targetOperation = &quot;auth-tokens&quot;)</Condition>
			<Request>
			</Request>   
			<Response>
			</Response>
        </Flow>
	</Flows>
    <PostFlow name="PostFlow">
        <Request>
            <Step>
                <Name>SF-PostflowRequestTargetLogAggregator</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>SF-PostflowResponseTargetLogAggregator</Name>
            </Step>
        </Response>
    </PostFlow>
    <HTTPTargetConnection>
    	<LoadBalancer>
            <Server name="Marriot-SwapTokens-Target">
                <IsEnabled>true</IsEnabled>
            </Server>
        </LoadBalancer>
        <Path>{targetBasePath}</Path>
    </HTTPTargetConnection>
</TargetEndpoint>
